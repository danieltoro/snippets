{
  "graphQLForComponent": {
    "prefix": "graphql",
    "body": ["import { compose, graphql } from 'react-apollo'", ""]
  },
  "exportGraphQL": {
    "prefix": "expgql",
    "body": [
      "export default compose(",
      "\tgraphql(${1:queryOrMutation}, { name: ${2:name} }),",
      ")(${1:${TM_FILENAME_BASE}})"
    ]
  }
}